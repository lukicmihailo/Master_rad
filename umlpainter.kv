#:kivy 1.8.0
#:import kivy kivy

<UMLPainterCanvas>:
	orientation: 'vertical'
	size:root.size
	BoxLayout:
		orientation: 'horizontal'
		size_hint_y: None
		id: menuID
		Spinner:
	 	   text: "Choose shapes"
	 	   values:("Basic shape","Links","Text","User Defined")
	 	   size_hint:(.4,1)
	 	   on_press: root.metoda1()
		Button:
	 	   text: "Button 2"
	 	   size_hint:None , None
	 	   on_press: root.metoda2()
		Button:
	 	   text: "Button 3"
	 	   size_hintss:(.4,1)
	 	   on_press: root.metoda3()
		Button:
	 	   text: "Button 4"
	 	   size_hint:(.4,1)
	 	   on_press: root.metoda4()
		Button:
	 	   text: "Button 5"
	 	   size_hintss:(.4,1)
	 	   on_press: root.metoda3()
    FloatLayout:
    	on_touch_down: root.metoda(self)
    	id: canvasID
    	size: root.size
    	canvas:
    		Color:
    			rgba: 1,1,1,1
    		Rectangle:
    			pos: self.pos
    			size: self.size
		Label:
			text:'nesto'
			fontsize: 100
    		
    		
    		
    		

