#:kivy 1.8.0
#:import kivy kivy

<UMLPainterFrame>:
	AnchorLayout:
		anchor_x: 'left'
		anchor_y: 'top'
		ToolBox:
			id: _tool_box
			size_hint: None,None
			width:100
	AnchorLayout:
		anchor_x: 'right'
		anchor_y: 'top'
		DrawingCanvas:
			size_hint: None,None
			width: root.width - _tool_box.width
			height: root.height - _toolbar.height - _status_bar.height
	AnchorLayout:
		anchor_x: 'center'
		anchor_y: 'bottom'
		BoxLayout:
			orientation: 'vertical'
			Toolbar:
				id: _toolbar
				size_hint: 1,None
				height: 48
			StatusBar:
				id: _status_bar
				size_hint: 1,None
				height: 24
				
		